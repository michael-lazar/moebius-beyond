{
    "name": "moebius-beyond",
    "version": "3.2.1",
    "description": "Modern ANSI Art Editor",
    "repository": {
        "type": "git",
        "url": "https://github.com/michael-lazar/moebius-beyond.git"
    },
    "main": "app/moebius.js",
    "scripts": {
        "start": "electron --no-sandbox --inspect=5858 .",
        "build": "npx electron-builder -mwl",
        "build-mac": "npx electron-builder -m",
        "build-win": "npx electron-builder -w",
        "build-linux": "npx electron-builder -l",
        "test": "npx playwright test --reporter=list",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "author": "Andy Herbert <andy.herbert@gmail.com>",
    "maintainers": [
        "Michael Lazar <lazar.michael22@gmail.com>"
    ],
    "license": "Apache-2.0",
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@playwright/test": "^1.54.2",
        "electron": "^37.2.5",
        "electron-builder": "^26.0.12",
        "eslint": "^9.31.0",
        "prettier": "^3.6.2"
    },
    "dependencies": {
        "@andreekeberg/imagedata": "^1.0.2",
        "@electron/remote": "^2.1.3",
        "json5": "^2.1.3",
        "upng-js": "^2.1.0"
    }
}
